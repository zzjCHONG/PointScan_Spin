<Application
    x:Class="Simscop.Spindisk.WPF.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:liftui="https://github.com/pchuan98/Lift.UI"
    xmlns:local="clr-namespace:Simscop.Spindisk.WPF">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <liftui:ThemeResources RequestedTheme="Dark" />
                <liftui:Theme />

                <ResourceDictionary Source="pack://application:,,,/Simscop.UI;component/Themes/Generic.xaml" />
                <ResourceDictionary Source="Styles/ObjectiveRadioButton.xaml" />
                <ResourceDictionary Source="Styles/CameraButton.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
